{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, {
      "extensions": [".js", ".jsx"]
    }],
    "jsx-a11y/label-has-associated-control": [2, {
      "labelComponents": ["label"],
      "controlComponents": ["input", "select"]
    }],
    "jsx-a11y/label-has-for": [2, {
      "required": {
        "every": ["id"]
      }
    }]
  },
  "env": {
    "browser": true,
    "jest": true
  }
}
